URxvt.perl-ext-common: default,clipboard,vtwheel,safe-paste,matcher,resize-font
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5
URxvt.url-launcher: /usr/bin/chromium
!! URXVT Resize-Font Settings
!!----------------------------------------------
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt.iso14755: false
URxvt.iso14755_52: false
!!----------------------------------------------
!! URXVT Copy Paste Settings
!!------------------------------------------------
URxvt.keysym.M-c: 			perl:clipboard:copy
URxvt.keysym.M-v: 			perl:clipboard:pasteq
!!------------------------------------------------
!! URXVT Matcher Settings 
!!------------------------------------------------
URxvt.keysym.C-Delete: perl:matcher:last
URxvt.keysym.M-Delete: perl:matcher:list
!!------------------------------------------------



URxvt.urgentOnBell: true
URxvt.scrollBar: false
URxvt.font: xft:SourceCodePro:pixelsize=13
!URxvt.transparent:	true
URxvt.shading:		15
! Fade the text by the given percentage when focus is lost
URxvt*fading:  30
! Turn on/off hold window after exit support. If enabled, urxvt will not immediately destroy its window !when the program executed within it exits. 
! Instead, it will wait till it is being killed or closed by the user.
URxvt*hold: true
! terminal padding
URxvt.internalBorder: 5
!URxvt.externalBorder: 0




!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull


